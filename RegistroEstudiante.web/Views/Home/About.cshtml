@model IEnumerable<RegistroEstudiante.data.Curso>
@{
    ViewBag.Title = "Cursos Disponibles";
}
<h2>@ViewBag.Title.</h2>
@*<h3>@ViewBag.Message</h3>*@

<div class="row">
    <div class="col-lg-12">
        <table class="table table-hover">
            <tr>
                <th>ID</th>
                <th>Nombre</th>
                <th>Objetivo</th>
                <th>Fecha de Apertura</th>
                <th>Facilitador</th>
            </tr>

            @foreach (var curso in Model)
            {


            <tr>
                <td> @curso.Id</td>
            <td>@curso.Nombre</td>
            <td>@curso.Objetivo</td>
            @*<td>@curso.FechaApertura</td>*@
            <td>@curso.NombreFacilitador</td>
            
            </tr>
            }

        </table>

    </div>


</div>

<p>Use this area to provide additional information.</p>
